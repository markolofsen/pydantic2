{
    "buildCommand": "pip install mkdocs-material mkdocs-minify-plugin && mkdocs build -f www/mkdocs.yml",
    "outputDirectory": "www/site",
    "framework": null,
    "builds": [
        {
            "src": "www/site/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/www/site/$1"
        }
    ]
}
